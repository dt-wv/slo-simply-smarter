{
  "name": "{{ .name }}",
  "rules": [
    {
      "enabled": true,
      "valueFormat": "{Host:Environment:env}",
      "valueNormalization": "Leave text as-is",
      "type": "ME",
      "attributeRule": {
        "entityType": "PROCESS_GROUP",
        "conditions": [
          {
            "key": "HOST_CUSTOM_METADATA",
            "dynamicKeySource": "ENVIRONMENT",
            "dynamicKey": "env",
            "operator": "EXISTS"
          }
        ],
        "pgToHostPropagation": true,
        "pgToServicePropagation": true
      }
    }
  ]
}
